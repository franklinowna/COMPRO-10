static void Main(string[] args)
        {
            string password;
            Console.Write("Enter the Password: ");
            password = Console.ReadLine();

            while (password != "frank")
            {

                Console.WriteLine("Invalid Password");


                Console.Write("Enter The Password: ");
                password = Console.ReadLine();
            }
            Console.Write("Access granted");
            Console.ReadKey();



        }
    }
}
